{
  "name": "registrations-service",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "npx ts-node -P tsconfig.nest.json src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/main.js",
    "start:dev": "npx ts-node -P tsconfig.nest.json src/main.ts",
    "start:prod": "node dist/src/main.js",
    "typeorm": "npx typeorm-ts-node-commonjs -d src/data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migrate:js": "node dist/run-migrations.js",
    "test": "jest --runInBand --passWithNoTests"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/throttler": "^6.0.0",
    "@nestjs/swagger": "^11.2.2",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "typeorm": "^0.3.27",
    "reflect-metadata": "^0.2.2",
    "pg": "^8.16.3",
    "axios": "^1.5.0",
    "@aurora/common": "file:../common",
    "class-validator": "^0.14.2",
    "class-transformer": "^0.5.1",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.4.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.0"
  },
  "description": "Registrations microservice",
  "license": "ISC",
  "type": "commonjs"
}
